{
    "name": "truktbackend",
    "version": "1.0.0",
    "description": "Backend for trukt app",
    "main": "main.js",
    "scripts": {
        "start": "node main.js",
        "test": "jest --coverage --detectOpenHandles --env=node --verbose  --forceExit",
        "test:unit": "jest --testPathPattern=jest/unit --passWithNoTests",
        "test:integration:db": "jest --testPathPattern=jest/integration/db --passWithNoTests",
        "encrypt": "bash ./tools/encrypt.sh",
        "decrypt": "bash ./tools/decrypt.sh",
        "setup": "bash ./tools/git_setup.sh && bash ./tools/decrypt.sh './local.settings.enc' './local.settings.json'",
        "merge-settings": "node ./tools/merge_settings.js",
        "copy-settings": "bash ./tools/decrypt.sh local.settings.enc local.settings.copy.json",
        "update-settings": "bash ./tools/encrypt.sh local.settings.copy.json local.settings.enc",
        "check-settings": "bash ./tools/decrypt.sh local.settings.enc local.settings.copy2.json && diff local.settings.copy2.json local.settings.copy.json",
        "postcheck-settings": "rm ./local.settings.copy2.json",
        "knex-debug": "set DEBUG=knex* && node main.js",
        "api-bundle": "openapi lint && openapi bundle main --output public/openapi/spec.yaml",
        "bundle-run": "npm run api-bundle && nodemon"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@azure/service-bus": "^7.3.0",
        "@azure/storage-blob": "^12.8.0",
        "@azure/web-pubsub": "^1.0.0-beta.3",
        "ajv": "^8.2.0",
        "ajv-keywords": "^5.0.0",
        "axios": "^0.21.4",
        "connect-session-knex": "^2.1.0",
        "cors": "^2.8.5",
        "currency.js": "^2.0.4",
        "express": "^4.17.1",
        "express-formidable": "^1.2.0",
        "express-openapi-validator": "^4.13.0",
        "express-session": "^1.17.2",
        "jsonwebtoken": "^8.5.1",
        "jwk-to-pem": "^2.0.5",
        "knex": "^0.95.6",
        "knex-postgis": "^0.14.1",
        "luxon": "^1.26.0",
        "mongodb": "^4.1.2",
        "multer": "^1.4.2",
        "node-cache": "^5.1.2",
        "node-cron": "^3.0.0",
        "objection": "^2.2.17",
        "pg": "^8.7.1",
        "pg-connection-string": "^2.5.0",
        "ramda": "^0.27.1",
        "us-state-codes": "^1.1.2",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@redocly/openapi-cli": "^1.0.0-beta.67",
        "@shelf/jest-mongodb": "^2.1.0",
        "axios-mock-adapter": "^1.19.0",
        "babel-eslint": "^10.1.0",
        "eslint": "^7.29.0",
        "eslint-plugin-import": "^2.22.1",
        "faker": "^5.5.3",
        "jest": "^26.6.3",
        "mock-knex": "^0.4.10",
        "nodemon": "^2.0.13",
        "yargs": "^17.1.0-candidate.0"
    }
}
