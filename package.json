        "connect-session-knex": "^2.1.0",
        "currency.js": "^2.0.4",
        "express": "^4.17.1",
        "express-formidable": "^1.2.0",
        "express-session": "^1.17.2",
        "knex": "^0.95.6",
        "luxon": "^1.26.0",
        "mongodb": "^3.6.6",
        "multer": "^1.4.2",
